/dts-v1/;

#include <dt-bindings/gpio/gpio.h>

#include "mcom03-elvmc03smarc-r2.2.dtsi"

/ {
	model = "ELV-MC03-SMARC r2.2, ELV-SMARC-CB r2.9";
	compatible = "elvees,elvmc03smarc-r2.2-elvsmarccb-r2.9",
		     "elvees,elvmc03smarc-elvsmarccb",
		     "elvees,elvsmarccb-r2.9",
		     "elvees,elvsmarccb",
		     "elvees,elvmc03smarc-r2.2",
		     "elvees,elvmc03smarc",
		     "elvees,mcom03";

	chosen {
		stdout-path = "serial0:115200n8";
		bootargs = "earlycon console=ttyS0,115200";
	};
};

&sdhci1 {
	status = "okay";

	/* Disable 1.8V support
	 * Disable SDR50, SDR104 and DDR50 support
	 */
	sdhci-caps-mask = <0x7 0x4000000>;
	sdhci-caps = <0x0 0x0>;
};

&ser1 {
	status = "okay";
};

&emac0 {
	status = "okay";
};

&emac1 {
	status = "okay";
};

/* USB3 SS */
&usb0 {
	status = "okay";
};

/*
* 2 hubs: 1 on ELV-MC03-SMARC and 1 on ELV-SMARC-CB
*/
&usb1 {
	status = "okay";
};

&pcie1 {
	status = "okay";
};
