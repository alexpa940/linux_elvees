/dts-v1/;

#include "mcom03.dtsi"

/ {
	model = "ELV-MC03-SMARC r1.0";
	compatible = "elvees,elvmc03smarc-r1.0",
		     "elvees,elvmc03smarc",
		     "elvees,mcom03";
};

&sdhci0 {
	status = "okay";

	/* Unstable at 25 MHz */
	max-frequency = <12000000>;
	non-removable;
	bus-width = <8>;

	/* HS, SDR50, SDR104 and DDR50 are not supported yet */
	sdhci-caps-mask = <0x7 0x200000>;
};
